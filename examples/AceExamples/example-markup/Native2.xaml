<Page xmlns:ace="using:run.ace" ace:Frame.Title="Page with ListBox">

    <ListBox ace:On.Loaded="addToListBox(this)">
        <ListBoxItem>Prime numbers starting after 1000</ListBoxItem>
    </ListBox>
    
    <Page.BottomAppBar>
        <ace:TabBar>
            <AppBarButton Label="Numbers" />
            <AppBarButton Label="Letters" />
        </ace:TabBar>
    </Page.BottomAppBar>
</Page>